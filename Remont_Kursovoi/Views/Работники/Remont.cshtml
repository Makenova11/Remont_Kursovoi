@model IEnumerable<Remont_Kursovoi.Remont_Result>

@{
    ViewBag.Title = "Index";
}

<h2>Ремонты работника</h2>

<table class="table">
    <tr>
        <th>
            Фамилия
        </th>
        <th>
            Имя
        </th>
        <th>
            Отчество
        </th>
        <th>
            
        </th>
        <th>
          
        </th>
        <th>
           
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Фамилия)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Имя)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Отчество)
            </td>

        </tr>
    }

</table>